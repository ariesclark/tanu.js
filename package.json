{
  "name": "tanu",
  "version": "0.2.0",
  "description": "Generate TypeScript types and interfaces using JavaScript or TypeScript code",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "ava",
    "test:coverage": "c8 ava"
  },
  "keywords": [
    "typescript",
    "compiler api",
    "abstraction",
    "wrapper",
    "tsc",
    "generation"
  ],
  "author": "Aries Clark <me@ariesclark.com> (https://ariesclark.com)",
  "license": "MIT",
  "devDependencies": {
    "@ariesclark/eslint-config": "^1.7.2",
    "@types/node": "^20.14.2",
    "ava": "^6.1.3",
    "c8": "^10.1.2",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}